<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Restaurante.gui.listagens.ControleTelaListarFuncionarios">
    <children>
        <ImageView fx:id="imgTop" fitHeight="448.0" fitWidth="653.0" layoutX="-6.0" layoutY="-12.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../imagens/wood-png-20.jpg" />
         </image></ImageView>
        <ListView fx:id="listView" layoutX="32.0" layoutY="41.0" prefHeight="288.0" prefWidth="541.0" visible="false" />
        <Button fx:id="botaoDeAcao" layoutX="207.0" layoutY="169.0" mnemonicParsing="false" onAction="#acaoBotaoListar" text="Listar">
            <font>
                <Font name="Arial Black" size="36.0" />
            </font></Button>
        <Label fx:id="labelDeAcao" layoutX="51.0" layoutY="84.0" text="Listar todos os Funcionarios:" textFill="#2c1313">
            <font>
                <Font name="Arial Bold" size="36.0" />
            </font></Label>
        <Button fx:id="btVoltar" layoutX="257.0" layoutY="348.0" mnemonicParsing="false" onAction="#acaoBotaoVoltar" text="Voltar" visible="false">
            <font>
                <Font name="Arial" size="12.0" />
            </font>
        </Button>
      <Button fx:id="btCancelar" layoutX="241.0" layoutY="347.0" mnemonicParsing="false" onAction="#acaoBotaoCancelar" text="Cancelar">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font></Button>
    </children>
</Pane>
