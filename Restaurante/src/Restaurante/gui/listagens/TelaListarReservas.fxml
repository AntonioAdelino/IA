<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Restaurante.gui.listagens.ControleTelaListarReservas">
    <children>
        <ImageView fx:id="imgTop" fitHeight="475.0" fitWidth="651.0" layoutX="-23.0" layoutY="-20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../imagens/wood-png-20.jpg" />
         </image></ImageView>
        <ListView fx:id="listView" layoutX="32.0" layoutY="41.0" prefHeight="288.0" prefWidth="541.0" visible="false" />
        <Button fx:id="botaoDeAcao" layoutX="219.0" layoutY="161.0" mnemonicParsing="false" onAction="#acaoBotaoListarReservas" text="Listar">
            <font>
                <Font name="Arial Black" size="36.0" />
            </font></Button>
        <Label fx:id="labelDeAcao" layoutX="9.0" layoutY="77.0" text="Listar todas as Reservas:">
            <font>
                <Font name="Arial Bold" size="48.0" />
            </font></Label>
        <Button fx:id="btVoltar" layoutX="272.0" layoutY="352.0" mnemonicParsing="false" onAction="#acaoBotaoCancelar" text="Voltar">
            <font>
                <Font name="Arial Bold" size="14.0" />
            </font>
        </Button>
    </children>
</Pane>
