<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="pane" prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Restaurante.gui.outros.ControleTelaRemovidoComSucesso">
    <children>
        <Button fx:id="btOk" layoutX="222.0" layoutY="94.0" mnemonicParsing="false" onAction="#acaoBotaoOk" text="OK">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font></Button>
        <Label fx:id="lbFinalizado" layoutX="137.0" layoutY="37.0" text="SUCESSO: Remoção completa!">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font></Label>
      <ImageView fx:id="imgTop" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../imagens/icones/aprovado.png" />
         </image></ImageView>
    </children>

</Pane>
